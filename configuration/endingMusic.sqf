/*
    Custom ending music.

    To add custom music to an ending, add it to the CfgMusic section in description.ext, and then replace nil with "YourMusicName".
    To remove music from an ending, replace nil with "".

    =====================================

    EXAMPLE (add custom music to ending #1 and #2, have no music on ending #3, and leave the rest with the default ending music):

    f_var_endingMusic_end1 = "MyOutro";
    f_var_endingMusic_end2 = "MyOutro";
    f_var_endingMusic_end3 = "";
    f_var_endingMusic_end4 = nil;
    f_var_endingMusic_end5 = nil;
    f_var_endingMusic_end6 = nil;

*/


f_var_endingMusic_end1 = nil;
f_var_endingMusic_end2 = nil;
f_var_endingMusic_end3 = nil;
f_var_endingMusic_end4 = nil;
f_var_endingMusic_end5 = nil;
f_var_endingMusic_end6 = nil;
